{
    "name": "chatfaq-widget",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "scripts": {
        "main": "dist/widget-loader.ssr.js",
        "browser": "dist/widget-loader.esm.js",
        "module": "dist/widget-loader.esm.js",
        "unpkg": "dist/widget-loader.min.js",
        "files": [
            "dist/*",
            "components/**/*.vue"
        ],
        "build": "nuxt build",
        "dev": "nuxt dev",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "package": "cross-env NODE_ENV=production rollup --config rollup.config.js",
        "package:ssr": "cross-env NODE_ENV=production rollup --config rollup.config.js --format cjs",
        "package:es": "cross-env NODE_ENV=production rollup --config rollup.config.js --format es",
        "package:unpkg": "cross-env NODE_ENV=production rollup --config rollup.config.js --format iife"
    },
    "devDependencies": {
        "@rollup/plugin-alias": "^4.0.3",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-replace": "^5.0.2",
        "cross-env": "^7.0.3",
        "minimist": "^1.2.8",
        "nuxt": "^3.1.1",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-scss": "^4.0.0",
        "rollup-plugin-terser": "^7.0.2"
    },
    "dependencies": {
        "@rollup/plugin-buble": "^1.0.2",
        "@rollup/plugin-commonjs": "^24.0.1",
        "primeflex": "^3.3.0",
        "primeicons": "^6.0.1",
        "primevue": "^3.23.0",
        "rollup-plugin-vue": "^6.0.0",
        "sass": "^1.58.0",
        "sass-loader": "^13.2.0"
    }
}
